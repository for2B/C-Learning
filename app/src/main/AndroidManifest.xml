        </activity>
        <activity android:name=".TestActivity">
        </activity>
        <activity android:name="com.test.file.BookDetail">
        </activity>
        <activity android:name="com.test.file.BookDirectory"
                  android:hardwareAccelerated="false" >
        </activity>
    </application>

</manifest>